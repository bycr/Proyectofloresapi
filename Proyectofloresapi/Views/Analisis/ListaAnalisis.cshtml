@{
    ViewBag.Title = "Lista Analisis";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@ViewBag.Title</h2>

<div class="row table-bordered">
    <div class="col-md-12">

        <div class="col-md-2 table-bordered">
            <label>Limite inferior</label>
            <br />
            @foreach (double linf in ViewBag.linf)
            {

                @linf
                <br />
            }
        </div>

        <div class="col-md-2 table-bordered">
            <label>Limite superior</label>
            <br />
            @foreach (double lsup in ViewBag.lsup)
            {

                @lsup
                <br />
            }
        </div>

        <div class="col-md-2 table-bordered">
            <label>Marca de clase</label>
            <br />
            @foreach (double marcaclase in ViewBag.marca_clase)
            {
                @marcaclase
                <br />
            }
        </div>


        <div class="col-md-2 table-bordered">
            <label>Frecuencia absoluta</label>
            <br />
            @foreach (double fabsol in ViewBag.fabsoluta)
            {
                @fabsol
                <br />
            }
        </div>

        <div class="col-md-2 table-bordered">
            <label>Frecuencia relativa</label>
            <br />
            @foreach (double frela in ViewBag.frelativa)
            {
                @frela
                <br />
            }
        </div>

        <div class="col-md-2 table-bordered">
            <label>XF</label>
            <br />
            @foreach (double xf in ViewBag.xf)
            {
                @xf
                <br />
            }
        </div>

    </div>

    <div class="col-md-12">
        <div class="col-md-2 table-bordered">
            <label>Media</label>
            <br />
            <label>@ViewBag.media</label>
        </div>

        <div class="col-md-2 table-bordered">
            <label>Moda</label>
            <br />
            <label>@ViewBag.moda</label>
        </div>


    </div>

</div>