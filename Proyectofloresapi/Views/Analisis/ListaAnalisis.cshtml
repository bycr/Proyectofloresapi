            </div>


            <div class="col-md-2 table-bordered">
                <label>@Recursos.Recurso.Analisis_fabsoluta</label>
                <br />
                @foreach (double fabsol in ViewBag.fabsoluta)
                {
                    @fabsol
                    <br />
                }
            </div>

            <div class="col-md-2 table-bordered">
                <label>Frecuencia relativa</label>
                <br />
                @foreach (double frela in ViewBag.frelativa)
                {
                    @frela
                    <br />
                }
            </div>

            <div class="col-md-2 table-bordered">
                @*Marca de clase * Frecuencia absoluta*@
                <label>Xi * Fi</label>
                <br />
                @foreach (double xf in ViewBag.xf)
                {
                    @xf
                    <br />
                }
            </div>

        </div>

        @*<div class="col-md-12">

            <div class="col-md-2">

            </div>

            <div class="col-md-2 table-bordered">
                <label>@Recursos.Recurso.Analisis_media</label>
                <br />
                <label>@ViewBag.media</label>
            </div>

            <div class="col-md-2 table-bordered">
                <label>@Recursos.Recurso.Analisis_mediana</label>
                <br />
                <label>@ViewBag.mediana</label>
            </div>

        </div>*@

        <br />
    </div>


    <div class="errabs row table-bordered">
        <div class="col-md-12">

            <div class="col-md-2 table-bordered">
                <label>@Recursos.Recurso.Analisis_proflores</label>
                <br />
